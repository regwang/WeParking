<!--location.wxml-->
<map id='shareMap' scale="16" class="map_style"  latitude="{{latitude}}" longitude="{{longitude}}"  markers="{{markers}}" />
<text wx:if="{{orderInfo.addressName}}" class='main_text location_desp'>{{orderInfo.addressName}}</text>
<text wx:wx:if="{{orderInfo.address}}" class='main_text location_desp'>{{orderInfo.address}}</text>
<text wx:if="{{orderInfo.addressRemark}}" class='main_text location_desp'>{{orderInfo.addressRemark}}</text>
